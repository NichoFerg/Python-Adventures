# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†

#Write your code below this line ðŸ‘‡

names = name1 + name2
lowercase_names = names.lower()

True_Total = str(lowercase_names.count("t") + lowercase_names.count("r") + lowercase_names.count("u") + lowercase_names.count("e"))

Love_Total = str(lowercase_names.count("l") + lowercase_names.count("o") + lowercase_names.count("v") + lowercase_names.count("e"))

love_score = int(True_Total + Love_Total)

if love_score >= 90 or love_score <= 10:
  print(f"Your score is {love_score}, you go together like coke and mentos.")
elif love_score >= 40 and love_score <= 50:
  print(f"Your score is {love_score}, you are alright together.")
else:
  print(f"Your score is {love_score}.")
